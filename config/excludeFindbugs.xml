<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter xmlns="https://github.com/spotbugs/filter/4.9.8"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="https://github.com/spotbugs/filter/4.9.8 https://raw.githubusercontent.com/spotbugs/spotbugs/4.9.8/spotbugs/etc/findbugsfilter.xsd">
    <Match>
        <Class name="com.dappermoose.batchlitcal.beans.CalendarOptions" />
        <Bug pattern="USBR_UNNECESSARY_STORE_BEFORE_RETURN" />
    </Match>
    <Match>
        <Class name="com.dappermoose.batchlitcal.calendar.OutputUtils" />
        <Bug pattern="EXS_EXCEPTION_SOFTENING_NO_CONSTRAINTS" />
    </Match>
    <Match>
        <Class name="com.dappermoose.batchlitcal.main.SpringConfig" />
        <Bug pattern="EXS_EXCEPTION_SOFTENING_NO_CONSTRAINTS" />
    </Match>
</FindBugsFilter>
